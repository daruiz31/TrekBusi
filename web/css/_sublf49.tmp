html body{
	font-family:'Raleway',sans-serif;
	font-size:16px;
	height:100%;
	margin:0;
	padding:0
}
h1{
	margin:0
}
header{
	width:100%;
	background:#26447f;
	position:fixed
	}
	header nav{
		width:100%;
		max-width:2000px;
		margin:0
	}

.bar-menu{
	display:none
}

img.icon-house{
	display:none
}

div.list-item{
	float: right;
	display: inline-block;
	/*margin-left:180px*/
}

header nav ul{
	overflow:hidden;
	margin:0;
	list-style:none
	}

	header nav ul li{
		display:inline-block

		}
	header ul li:hover{
				background:#2683cb;
				-webkit-transition:background-color .3s
				}

		header ul li.item-hover:hover{
			background:#d09411;
			-webkit-transition:background-color .3s
		}

		header ul li.item-hover2:hover{
			background:#d09411;
			-webkit-transition:background-color .3s
		}

		header nav ul li a{
			color:#fff;
			padding:20px;
			font-size:16px;
			display:block;
			text-decoration:none
			}

			header nav ul li span{
				margin-rigth:10px

				}

@media screen and (max-width:1024px){
	header nav{
		width:100%;
		height:100%;
		left:-100%;
		margin:65px 0px 0px 0px;
		position:fixed;
		background:#26447f
		}

		header nav ul{
			padding:0
		}

			header nav ul li{
				display:block;
				float:none
			}

			div.list-item{
				float: left;
				display: inline-block;
				/*margin-left:180px*/	
			}
				header nav ul li img{
					display:inline-block;
					padding:0 20px 0 0;
				}

				header nav ul li a{
					color:#fff;
					padding:20px;
					font-size:18px;
					display:block;
					text-decoration:none
				}
		img.icon-house{
			display:inline-block
		}

		li.list-item{
			float:right;
			margin:0
		}

		li.list-item2{
			float:right;
			margin:0
		}
		

		.bar-menu{
			display:block;
			width:100%;
			background:#26447f;
			color:#fff;
			position:fixed;
			text-decoration:none
		}
			.bar-menu .icon-menu{
				display:block;
				padding:20px;
				color:#fff;
				text-decoration:none
		}
			.bar-menu a{
				text-decoration:none;
				font-size:25px;
				-webkit-box-sizing:border-box;
				-moz-box-sizing:border-box;
				box-sizing:border-box
			}

}

